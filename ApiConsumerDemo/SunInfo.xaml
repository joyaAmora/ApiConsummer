<Window x:Class="ApiConsumerDemo.SunInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiConsumerDemo"
        mc:Ignorable="d"
        Title="SunInfo" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <StackPanel>
                <Button x:Name="loadSunInfo" Margin="20" Click="loadSunInfo_Click">
                    Telecharger les informations
                </Button>
                <TextBlock x:Name="sunRiseText" Margin="20"></TextBlock>
                <TextBlock x:Name="sunSetText" Margin="20"></TextBlock>
            </StackPanel>
        </DockPanel>
        
    </Grid>
</Window>
